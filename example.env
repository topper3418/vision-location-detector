# Vision Location Detector Configuration

# Device Configuration
# Use 'cuda' or '0' for GPU, 'cpu' for CPU
DEVICE=cuda

# YOLO Configuration
# Set to 'true' to enable YOLO object detection
ENABLE_YOLO=true
# Set to 'true' to use TensorRT for YOLO inference (only applicable if DEVICE is set to 'cuda' and device supports TensorRT (NVIDIA GPUs))
USE_TENSORRT=true
YOLO_MODEL_PATH=yolov8n.pt
CONFIDENCE_THRESHOLD=0.5

# Camera Configuration
CAMERA_ID=0
CAMERA_WIDTH=640
CAMERA_HEIGHT=480

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8080

# Performance Settings
JPEG_QUALITY=50
